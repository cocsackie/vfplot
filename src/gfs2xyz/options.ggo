package "gfs2xyz2D"
purpose "extract ascii grids of scalar fields in gfs files"

option "index"		i	"write i,j indices"		flag	off
option "output"		o	"output to file"		string	no
option "scalar"		s	"name of scalar variable"	string	default="P" no
option "sag"		S	"output data in sag(5) format"	flag	off	
option "verbose"	v	"verbose"			flag	off

text "\nSee gfs2xyz2D(1) for more details"
